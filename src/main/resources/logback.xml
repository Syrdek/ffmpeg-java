<configuration>

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>
  
  <logger name="fr.gouv.mjl.dune.ffmpeg.java.io.AVIO" level="info"/>
  <logger name="fr.gouv.mjl.dune.ffmpeg.java.io.stream.JAVStream" level="info"/>

  <root level="debug">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>